template <class T>
class node{
    int element;
    int *next;
    friend class singlelist<T>;
    };

class singlelist{
    singlelist(head=NULL;value=0);
    ~singlelist();
    bool find(int x);
    int search(int x);
    bool insert(int n,int x);
    bool delete(int n,int x);
    void clear();
    int getlength();
    void output();
private:
    node *first;
    }

    //node* delete(int n);
